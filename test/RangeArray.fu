public class Test
{
	public static bool Run()
	{
		0 .. 9 [3] digits;
		digits[0] = 1;
		digits[1] = 3;
		digits[2] = 7;
		-10 .. 10 [2] a;
		a[0] = -5;
		a[1] = 7;
		0 .. 30_000 [2] usa;
		usa[0] = 30_000;
		-40_000 .. (-1000) [2] sia;
		sia[1] = -35_000;
		return digits[0] == 1 && digits[1] == 3 && digits[2] == 7
			&& a[0] == -5 && a[1] == 7
			&& usa[0] == 30_000
			&& sia[1] == -35_000;
	}
}
