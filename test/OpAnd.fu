public static class Test
{
	public static bool Run()
	{
		int a = 0xa;
		int b = 0x12;
		bool c = true;
		return (a & b) == 2
			&& ((c ? a : b) & 8) == 8
			&& (8 & (c ? a : b)) == 8
			&& ((a | b) & 0x18) == 0x18
			&& (0x18 & (a | b)) == 0x18
			&& ((a ^ b) & 0x10) == 0x10
			&& (0x18 & (a ^ b)) == 0x18
			&& (a & b & 6) == 2
			&& (a & (b & 7)) == 2
			&& ((a << 1) & b) == 0x10
			&& (a & (b << 2)) == 8
			&& ((a >> 1) & b) == 0
			&& (a & (b >> 1)) == 8
			&& ((a + b) & 0x14) == 0x14
			&& (a & (b + 2)) == 0
			&& ((b - a) & 8) == 8
			&& (12 & (a - 2)) == 8
			&& ((a * b) & 0xc4) == 0x84
			&& (b & (a * 2)) == 0x10
			&& ((b / a) & 3) == 1
			&& (5 & (b / a)) == 1
			&& ((b % a) & 12) == 8
			&& (9 & (b % a)) == 8;
	}
}
